<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ServiceCallout async="false" continueOnError="false" enabled="true" name="Partner-Baas">
    <DisplayName>Partner Baas</DisplayName>
    <FaultRules/>
    <Properties/>
    <Request clearPayload="true" variable="myRequest">
        <Set>
            <Headers>
                <Header name="Accept">application/json</Header>
            </Headers>
            <Payload contentType="application/json">
            {
&quot;company_name&quot;:&quot;{request.formparam.company_name}&quot;,
&quot;sgs_partner&quot;:&quot;true&quot;,
&quot;start_date&quot;:&quot;{request.formparam.start_date}&quot;,
&quot;end_date&quot;:&quot;{request.formparam.end_date}&quot;,
&quot;number_of_units&quot;:&quot;{request.formparam.number_of_units}&quot;,
&quot;load_shed+program&quot;:&quot;{request.formparam.load_shed_program}&quot;,
&quot;unit_billed_on&quot;:&quot;{request.formparam.units_billed}&quot;,
&quot;install_fee_per_unit&quot;:&quot;{request.formparam.instal_fee_per_unit}&quot;,
&quot;fee_per_unit&quot;:&quot;{request.formparam.fee_per_unit_year}&quot;,
&quot;ee_participation&quot;:&quot;{request.formparam.ee_participation}&quot;,
&quot;ee_fee_per_unit&quot;:&quot;{request.formparam.ee_fee_per_unit}&quot; 
}
        </Payload>
            <Verb>POST</Verb>
        </Set>
        <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
    </Request>
    <Response>calloutResponse</Response>
    <HTTPTargetConnection>
        <Properties/>
        <URL>https://api.usergrid.com/shridhar.nagaral/sandbox/partnerwebdata</URL>
    </HTTPTargetConnection>
</ServiceCallout>
