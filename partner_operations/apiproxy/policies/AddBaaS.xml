<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ServiceCallout async="false" continueOnError="false" enabled="true" name="AddBaaS">
    <DisplayName>AddBaaS</DisplayName>
    <FaultRules/>
    <Properties/>
    <Request clearPayload="true" variable="myRequest">
        <Set>
            <Headers>
                <Header name="Accept">application/json</Header>
            </Headers>
            <Payload contentType="application/json" variablePrefix="@" variableSuffix="#">@result#</Payload>
            <Verb>POST</Verb>
        </Set>
        <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    </Request>
    <Response>calloutResponse</Response>
    <HTTPTargetConnection>
        <Properties/>
        <URL>https://{hch_baas}/{partner_baas}</URL>
    </HTTPTargetConnection>
</ServiceCallout>
